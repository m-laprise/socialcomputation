  keywords = {brain machine interfaces,dimensionality reduction,gain modulation,neural data analysis,neural networks (recurrent),TCA},
  file = {/Users/mlaprise/Zotero/storage/LFCGMQ6S/Williams et al_2018_Unsupervised Discovery of Demixed, Low-Dimensional Neural Dynamics across.pdf}
}

