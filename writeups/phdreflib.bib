  file = {/Users/mlaprise/Zotero/storage/AFXPC7PT/Higgins et al_2020_The Bright but Modest Potential of Algorithms in the Courtroom.pdf;/Users/mlaprise/Zotero/storage/YFAF5ZVA/321992462.html}
