  file = {/Users/mlaprise/Zotero/storage/ZNS7H33Z/Palladino_2020_Do corporate insiders use stock buybacks for personal gain.pdf;/Users/mlaprise/Zotero/storage/Z2TFQ72B/02692171.2019.html}
