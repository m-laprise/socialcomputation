}

@article{hackerWinnerTakeAllPoliticsPublic2010,
  title = {Winner-{{Take-All Politics}}: {{Public Policy}}, {{Political Organization}}, and the {{Precipitous Rise}} of {{Top Incomes}} in the {{United States}}},
