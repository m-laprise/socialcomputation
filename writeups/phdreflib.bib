@article{millanLocalTopologicalMoves2021,
  title = {Local {{Topological Moves Determine Global Diffusion Properties}} of {{Hyperbolic Higher-Order Networks}}},
  author = {Millán, Ana P. and Ghorbanchian, Reza and Defenu, Nicolò and Battiston, Federico and Bianconi, Ginestra},
  date = {2021},
  journaltitle = {Physical Review E},
