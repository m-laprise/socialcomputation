@article{cividinoMultiassetFinancialBubbles2023,
  title = {Multiasset Financial Bubbles in an Agent-Based Model with Noise Traders' Herding Described by an \$n\$-Vector Ising Model},
  author = {Cividino, Davide and Westphal, Rebecca and Sornette, Didier},
  date = {2023-01-10},
  journaltitle = {Physical Review Research},
  shortjournal = {Phys. Rev. Res.},
