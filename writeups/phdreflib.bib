@inproceedings{habererConsumersBenefitSelling2022,
  title = {Do {{Consumers Benefit}} from {{Selling}} Their {{Data}}? {{The Economic Effects}} of {{Personal Data Brokers}} in {{Digital Markets}}},
  shorttitle = {Do {{Consumers Benefit}} from {{Selling}} Their {{Data}}?},
