@article{waldmanManufacturedUncertaintyConstitutional2022,
  title = {Manufactured {{Uncertainty}} in {{Constitutional Law}}},
  author = {Waldman, Ari Ezra},
  date = {2022-09-06},
  journaltitle = {Fordham Law Review},
  volume = {91},
  doi = {10.2139/ssrn.4211548},
  url = {https://papers.ssrn.com/abstract=4211548},
  urldate = {2023-09-25},
  abstract = {Civil rights litigation is awash in misinformation. Courts have found or litigants have argued that abortion causes cancer, that adolescent gender-affirming hormone therapy is irreversible, that in-person voter fraud is a massive problem. Except none of that is true. The conventional scholarly account about law and misinformation, disinformation, and dubious claims of fact focuses the power of legislatures and amici to engage in perfunctory fact-finding and to rely on “alternative facts” or outright falsehoods to justify laws that harm and restrict the rights of marginalized populations. At the same time, the literature suggests that judges and the law are inundated with uninterrogated claims, incapable of sifting through the muck, and held hostage by rules of deference. So framed, the conventional account proposes to empower judges to interrogate the processes of legislative fact-finding and the factual predicates of legislative action.This Article challenges that account. It demonstrates that the problem of misinformation in the law is not limited to legislatures and amici. Rather, the problem is built into judging and into doctrine itself. This Article argues that in fact-intensive constitutional cases, opportunistic judges manufacture factual uncertainty where there is none by reframing doctrines of constitutional scrutiny as demands for scientific infallibility. And because few, if any, scientific studies are perfect, this strategy opens the door for any study, any screed, or any claim, no matter how dubious or unproven, to put rights at risk. The result is not just uncertainty, chaos, and indeterminacy. This pattern of doctrinal collapse routinely and consistently serves to reify traditional hierarchies of power.Learning lessons from the legal realist and critical legal studies traditions, this Article reframes the problem of misinformation in constitutional litigation from a narrow focus on fact-checking to a broader problem of legal doctrine and judicial power. Therefore, contrary to the current literature, which seeks to give judges more power to distinguish among factual claims, this Article takes a more holistic approach, focusing on political economic, doctrinal, and institutional change.},
  langid = {english},
  keywords = {_à résumer,_pertinent,abortion,constitutional law,legislative facts,LGBTQ,misinformation,social facts,transgender,voter ID laws,voting},
  annotation = {0 citations (Semantic Scholar/DOI) [2023-09-25]},
  file = {/Users/mlaprise/Zotero/storage/J2AX37U7/Waldman_2022_Manufactured Uncertainty in Constitutional Law.pdf}
}

