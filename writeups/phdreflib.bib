@incollection{deakinCorporationLegalStudies2017,
  title = {The {{Corporation}} in {{Legal Studies}}},
  booktitle = {The {{Corporation}}: {{A Critical}}, {{Multi-Disciplinary Handbook}}},
  author = {Deakin, Simon},
  editor = {Spicer, Andre and Baars, Grietje},
  date = {2017},
